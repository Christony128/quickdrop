

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Upload{
  id  String @id @default(cuid())
  name  String
  url   String
  fileType String
  fileSize Int
  createdAt DateTime @default(now())
  expiresAt DateTime?
  @@map("uploads")
}